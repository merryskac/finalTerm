{
  "version":2,
  "builds":[
    {
      "src":"be/backend/api/server.js",
      "use":"@vercel/node"
    }
  ],
  "routes":[
    {
      "src":"/(.*)",
      "dest":"be/backend/api/server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "http://192.168.1.8:5173/",
        "Access-Control-Allow-Credentials":"true"
      }
    }
  ]
  
}